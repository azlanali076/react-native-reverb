{
  "name": "react-native-reverb",
  "version": "1.0.2",
  "description": "React Native TurboModule wrapper for Laravel Reverb WebSocket",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "react-native": "index.ts",
  "codegenConfig": {
    "name": "NativeReverbSpec",
    "type": "modules",
    "jsSrcsDir": "src/specs",
    "android": {
      "javaPackageName": "com.flamesware.nativereverb"
    },
    "ios": {
      "modulesProvider": {
        "NativeReverb": "NativeReverb"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "keywords": [
    "react-native",
    "turbo-module",
    "laravel",
    "reverb",
    "websocket",
    "bridge",
    "ios",
    "android"
  ],
  "peerDependencies": {
    "react": "^19.1.1",
    "react-native": "^0.81.4"
  },
  "devDependencies": {
    "@types/react-native": "^0.73.0",
    "typescript": "^5.0.0"
  },
  "author": {
    "name": "Syed Azlan Ali",
    "email": "azlanali076@gmail.com",
    "url": "https://github.com/azlanali076"
  },
  "license": "MIT",
  "homepage": "https://github.com/azlanali076/react-native-reverb#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azlanali076/react-native-reverb.git"
  },
  "bugs": {
    "url": "https://github.com/azlanali076/react-native-reverb/issues"
  }
}
